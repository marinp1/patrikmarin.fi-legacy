{
  "name": "patrimarin.fi_server",
  "version": "1.0.0",
  "scripts": {
    "lint": "tslint 'src/**/*.ts?(x)'",
    "test": "jest",
    "watch": "nodemon --watch 'src/server.ts' --exec 'ts-node' src/server.ts",
    "start": "ts-node src/server.ts"
  },
  "dependencies": {
    "contentful": "^5.0.5",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "express-redis-cache": "^1.0.0",
    "ts-node": "^4.1.0",
    "typescript": "^2.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.11.0",
    "jest": "^22.0.4",
    "ts-jest": "^22.0.0",
    "tslint": "^5.8.0",
    "tslint-config-airbnb": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/__tests__/.*\\.(ts|tsx|js)$"
  }
}
